<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.0"
              xmlns="http://java.sun.com/xml/ns/javaee" 
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
    <application>
        <resource-bundle>
            <base-name>/Bundle</base-name>
            <var>bundle</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/Bundle</base-name>
            <var>bundle</var>
        </resource-bundle>
        
    </application>
    
    <navigation-rule>
        <from-view-id>/*</from-view-id>
        <navigation-case>
            <from-outcome>header_poll_navigation</from-outcome>
            <to-view-id>/polls/List.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>header_poll_question_navigation</from-outcome>
            <to-view-id>/pollQuestions/List.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>header_poll_answer_navigation</from-outcome>
            <to-view-id>/pollAnswers/List.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>header_poll_filling_navigation</from-outcome>
            <to-view-id>/pollFillings/List.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>poll_question_navigation</from-outcome>
            <to-view-id>/pollQuestions/View.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>poll_navigation</from-outcome>
            <to-view-id>/polls/View.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>poll_answer_navigation</from-outcome>
            <to-view-id>/pollAnswers/View.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    

</faces-config>
